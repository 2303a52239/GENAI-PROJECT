    {
      "cell_type": "code",
      "source": [
        "import numpy as np\n",
        "from tensorflow.keras.models import Sequential\n",
        "from tensorflow.keras.layers import Embedding, Conv1D, GlobalMaxPooling1D, Dense\n",
        "from tensorflow.keras.preprocessing.text import Tokenizer\n",
        "from tensorflow.keras.preprocessing.sequence import pad_sequences\n",
        "from sklearn.preprocessing import LabelEncoder\n",
        "\n",
        "# Sample medical report data\n",
        "texts = [\n",
        "    \"Patient has hypertension and diabetes\",\n",
        "    \"Blood pressure is normal, no heart issues\",\n",
        "    \"Brain scan shows signs of memory loss\"\n",
        "]\n",
        "summaries = [\"diabetes\", \"normal\", \"memory_loss\"]  # only 1 word per summary for simplicity\n",
        "\n",
        "# Tokenize input text\n",
        "tokenizer = Tokenizer()\n",
        "tokenizer.fit_on_texts(texts)\n",
        "vocab_size = len(tokenizer.word_index) + 1\n",
        "X = pad_sequences(tokenizer.texts_to_sequences(texts), maxlen=10)\n",
        "\n",
        "# Encode summaries as labels\n",
        "label_encoder = LabelEncoder()\n",
        "y = label_encoder.fit_transform(summaries)\n",
        "\n",
        "# Build a simplified CNN model for classification\n",
        "model = Sequential([\n",
        "    Embedding(input_dim=vocab_size, output_dim=50, input_length=10),\n",
        "    Conv1D(64, 5, activation='relu'),\n",
        "    GlobalMaxPooling1D(),\n",
        "    Dense(32, activation='relu'),\n",
        "    Dense(len(label_encoder.classes_), activation='softmax')\n",
        "])\n",
        "\n",
        "model.compile(optimizer='adam', loss='sparse_categorical_crossentropy', metrics=['accuracy'])\n",
        "model.summary()\n",
        "\n",
        "# Train the model\n",
        "model.fit(X, y, epochs=10, verbose=1)\n",
        "\n",
        "# Test on a new medical report\n",
        "new_text = [\"Patient diagnosed with diabetes and kidney problems\"]\n",
        "X_new = pad_sequences(tokenizer.texts_to_sequences(new_text), maxlen=10)\n",
        "predicted_label = label_encoder.inverse_transform(np.argmax(model.predict(X_new), axis=1))\n",
        "\n",
        "print(\"\\n✅ Predicted summary keyword:\", predicted_label[0])\n"
      ],
